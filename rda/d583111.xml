          <gmd:title>
            <gco:CharacterString>WRF-Chem Simulations of Dust Storms With and Without Inserting SMAP Soil Moisture Retrievals</gco:CharacterString>
          </gmd:title>
          <gmd:alternateTitle>
            <gco:CharacterString>d583111</gco:CharacterString>
